package day1;
class p93 
{
	public static void main(String[] args) 
	{

			String str1 ="hello";
			String str2 ="World";
			str1 += str2;
			for(int i=str1.length()-1; i>=0; i--){
				if(i<=str2.length()){
					str2 =+str1.charAt(i)+"";
				} else{
					str1 =str1.charAt(i)+"";
				}
			}

			System.out.println("str1 :" + str1);
			System.out.println("str2 :" + str2);


		
	}
}
